<project>

    <target name="clean">
        <delete dir="build"/>
    </target>

    <target name="compile">
        <mkdir dir="build/classes"/>
        <javac srcdir="src" destdir="build/classes"/>
    </target>

    <target name="jar">
        <mkdir dir="."/>
        <jar destfile="fibo.jar" basedir="build/classes">
            <manifest>
                <attribute name="Main-Class" value="fibo"/>
            </manifest>
        </jar>

    </target>
</project>
